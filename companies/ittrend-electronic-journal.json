b\u00d0\u00b5\u00d0\u00ba\u00d1\u201a\u00d1\u0192\u00d1\u017d\u00d1\u2030\u00d0\u00b8\u00d0\u00b5\u00c2\u00bb, \u00c2\u00ab\u00d0\u02dc\u00d1\u0081\u00d1\u201a\u00d0\u00be\u00d1\u20ac\u00d0\u00b8\u00d1\u008f \u00d0\u00b8 \u00d0\u009d\u00d0\u00b0\u00d1\u0192\u00d0\u00ba\u00d0\u00b0\u00c2\u00bb, \u00c2\u00ab\u00d0\u0178\u00d1\u20ac\u00d0\u00be\u00d0\u00b3\u00d1\u20ac\u00d0\u00b0\u00d0\u00bc\u00d0\u00bc\u00d1\u2039 \u00d0\u00b8 \u00d0\u02dc\u00d0\u00b3\u00d1\u20ac\u00d1\u2039\u00c2\u00bb. \u00d0\u0161\u00d0\u00b0\u00d0\u00b6\u00d0\u00b4\u00d0\u00b0\u00d1\u008f \u00d0\u00b8\u00d0\u00b7 \u00d1\u20ac\u00d1\u0192\u00d0\u00b1\u00d1\u20ac\u00d0\u00b8\u00d0\u00ba \u00d1\u0081\u00d0\u00be\u00d0\u00b4\u00d0\u00b5\u00d1\u20ac\u00d0\u00b6\u00d0\u00b8\u00d1\u201a \u00d0\u00bd\u00d0\u00be\u00d0\u00b2\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d0\u00b8 \u00d1\u0081\u00d0\u00be\u00d0\u00be\u00d1\u201a\u00d0\u00b2\u00d0\u00b5\u00d1\u201a\u00d1\u0081\u00d1\u201a\u00d0\u00b2\u00d1\u0192\u00d1\u017d\u00d1\u2030\u00d0\u00b5\u00d0\u00b9 \u00d1\u201a\u00d0\u00b5\u00d0\u00bc\u00d0\u00b0\u00d1\u201a\u00d0\u00b8\u00d0\u00ba\u00d0\u00b8, \u00d1\u0081\u00d1\u201a\u00d0\u00b0\u00d1\u201a\u00d1\u0152\u00d0\u00b8, \u00d1\u20ac\u00d0\u00b5\u00d0\u00bf\u00d0\u00be\u00d1\u20ac\u00d1\u201a\u00d0\u00b0\u00d0\u00b6\u00d0\u00b8, \u00d0\u00b8\u00d0\u00bd\u00d1\u201a\u00d0\u00b5\u00d1\u20ac\u00d0\u00b2\u00d1\u0152\u00d1\u017d.\u003C/p\u003E\n\n\u003Cp\u003E\u00d0\u017e\u00d1\u0081\u00d0\u00bd\u00d0\u00be\u00d0\u00b2\u00d0\u00bd\u00d0\u00be\u00d0\u00b9 \u00d0\u00be\u00d1\u0081\u00d0\u00be\u00d0\u00b1\u00d0\u00b5\u00d0\u00bd\u00d0\u00bd\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d1\u0152\u00d1\u017d \u00d0\u00bd\u00d0\u00b0\u00d1\u02c6\u00d0\u00b5\u00d0\u00b3\u00d0\u00be \u00d0\u00b6\u00d1\u0192\u00d1\u20ac\u00d0\u00bd\u00d0\u00b0\u00d0\u00bb\u00d0\u00b0 \u00d1\u008f\u00d0\u00b2\u00d0\u00bb\u00d1\u008f\u00d0\u00b5\u00d1\u201a\u00d1\u0081\u00d1\u008f \u00d1\u0081\u00d0\u00bf\u00d0\u00be\u00d1\u0081\u00d0\u00be\u00d0\u00b1 \u00d0\u00b5\u00d0\u00b3\u00d0\u00be \u00d1\u20ac\u00d0\u00b0\u00d1\u0081\u00d0\u00bf\u00d1\u20ac\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d1\u20ac\u00d0\u00b0\u00d0\u00bd\u00d0\u00b5\u00d0\u00bd\u00d0\u00b8\u00d1\u008f \u00e2\u20ac\u201c \u00d0\u00b6\u00d1\u0192\u00d1\u20ac\u00d0\u00bd\u00d0\u00b0\u00d0\u00bb \u00d0\u00b0\u00d0\u00b1\u00d1\u0081\u00d0\u00be\u00d0\u00bb\u00d1\u017d\u00d1\u201a\u00d0\u00bd\u00d0\u00be \u00d0\u00b1\u00d0\u00b5\u00d1\u0081\u00d0\u00bf\u00d0\u00bb\u00d0\u00b0\u00d1\u201a\u00d0\u00bd\u00d0\u00be \u00d1\u20ac\u00d0\u00b0\u00d1\u0081\u00d0\u00bf\u00d1\u20ac\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d1\u20ac\u00d0\u00b0\u00d0\u00bd\u00d1\u008f\u00d0\u00b5\u00d1\u201a\u00d1\u0081\u00d1\u008f \u00d0\u00b1\u00d0\u00be\u00d0\u00bb\u00d0\u00b5\u00d0\u00b5 \u00d1\u2021\u00d0\u00b5\u00d0\u00bc \u00d0\u00b2 5,000 \u00d0\u00be\u00d1\u20ac\u00d0\u00b3\u00d0\u00b0\u00d0\u00bd\u00d0\u00b8\u00d0\u00b7\u00d0\u00b0\u00d1\u2020\u00d0\u00b8\u00d1\u008f\u00d1\u2026, \u00d0\u00bc\u00d1\u2039 \u00d0\u00b2\u00d1\u2039\u00d1\u2026\u00d0\u00be\u00d0\u00b4\u00d0\u00b8\u00d0\u00bc \u00d0\u00b2 \u00d0\u00b4\u00d0\u00b2\u00d1\u0192\u00d1\u2026 \u00d0\u00b2\u00d0\u00b5\u00d1\u20ac\u00d1\u0081\u00d0\u00b8\u00d1\u008f\u00d1\u2026: \u00d0\u00bd\u00d0\u00b0 CD \u00d0\u00b8/\u00d0\u00b8\u00d0\u00bb\u00d0\u00b8 DVD \u00d0\u00b4\u00d0\u00b8\u00d1\u0081\u00d0\u00ba\u00d0\u00b0\u00d1\u2026, \u00d0\u00b0 \u00d1\u201a\u00d0\u00b0\u00d0\u00ba\u00d0\u00b6\u00d0\u00b5 \u00d0\u00bf\u00d1\u20ac\u00d0\u00b5\u00d0\u00b4\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d0\u00b0\u00d0\u00b2\u00d0\u00bb\u00d1\u008f\u00d0\u00b5\u00d0\u00bc \u00d0\u00b2\u00d0\u00be\u00d0\u00b7\u00d0\u00bc\u00d0\u00be\u00d0\u00b6\u00d0\u00bd\u00d0\u00be\u00d1\u0081\u00d1\u201a\u00d1\u0152 \u00d0\u00bf\u00d1\u20ac\u00d0\u00be\u00d1\u0081\u00d0\u00bc\u00d0\u00be\u00d1\u201a\u00d1\u20ac\u00d0\u00b0 \u00d0\u00b6\u00d1\u0192\u00d1\u20ac\u00d0\u00bd\u00d0\u00b0\u00d0\u00bb\u00d0\u00b0 \u00d0\u00b2 \u00d1\u20ac\u00d0\u00b5\u00d0\u00b6\u00d0\u00b8\u00d0\u00bc\u00d0\u00b5 \u00d0\u00be\u00d0\u00bd\u00d0\u00bb\u00d0\u00b0\u00d0\u00b9\u00d0\u00bd \u00d0\u00b8 \u00d1\u0081\u00d0\u00ba\u00d0\u00b0\u00d1\u2021\u00d0\u00b8\u00d0\u00b2\u00d0\u00b0\u00d0\u00bd\u00d0\u00b8\u00d1\u008f \u00d1\u0081 \u00d0\u00bd\u00d0\u00b0\u00d1\u02c6\u00d0\u00b5\u00d0\u00b3\u00d0\u00be \u00d1\u0081\u00d0\u00b0\u00d0\u00b9\u00d1\u201a\u00d0\u00b0.\u003C/p\u003E\n\n\u003Cp\u003E\u00d0\u00a1\u00d0\u0090\u00d0\u2122\u00d0\u00a2 ittrend.am \u00d0\u00b1\u00d1\u2039\u00d0\u00bb \u00d1\u0081\u00d0\u00be\u00d0\u00b7\u00d0\u00b4\u00d0\u00b0\u00d0\u00bd \u00d0\u00b2 2010 \u00d0\u00b3\u00d0\u00be\u00d0\u00b4\u00d1\u0192  \u00d0\u00b8 \u00d0\u00bd\u00d0\u00b0\u00d0\u00bc\u00d0\u00b5\u00d1\u20ac\u00d0\u00b5\u00d0\u00bd \u00d0\u00bf\u00d1\u20ac\u00d0\u00b8\u00d0\u00be\u00d0\u00b1\u00d1\u20ac\u00d0\u00b5\u00d1\u0081\u00d1\u201a\u00d0\u00b8 \u00d0\u00bf\u00d1\u20ac\u00d0\u00b8\u00d0\u00b7\u00d0\u00bd\u00d0\u00b0\u00d0\u00bd\u00d0\u00b8\u00d0\u00b5 \u00d0\u00bf\u00d1\u20ac\u00d0\u00be\u00d1\u201e\u00d0\u00b5\u00d1\u0081\u00d1\u0081\u00d0\u00b8\u00d0\u00be\u00d0\u00bd\u00d0\u00b0\u00d0\u00bb\u00d0\u00be\u00d0\u00b2 \u00d0\u02dc\u00d0\u00a2 \u00d0\u00be\u00d1\u201a\u00d1\u20ac\u00d0\u00b0\u00d1\u0081\u00d0\u00bb\u00d0\u00b8 \u00d0\u00b8 \u00d1\u02c6\u00d0\u00b8\u00d1\u20ac\u00d0\u00be\u00d0\u00ba\u00d0\u00be\u00d0\u00b9 \u00d1\u2021\u00d0\u00b8\u00d1\u201a\u00d0\u00b0\u00d1\u201a\u00d0\u00b5\u00d0\u00bb\u00d1\u0152\u00d1\u0081\u00d0\u00ba\u00d0\u00be\u00d0\u00b9 \u00d0\u00b0\u00d1\u0192\u00d0\u00b4\u00d0\u00b8\u00d1\u201a\u00d0\u00be\u00d1\u20ac\u00d0\u00b8\u00d0\u00b8.\u003C/p\u003E\n\n\u003Cp\u003E\u00d0\u2022\u00d0\u00b6\u00d0\u00b5\u00d0\u00b4\u00d0\u00bd\u00d0\u00b5\u00d0\u00b2\u00d0\u00bd\u00d0\u00be \u00d0\u00bd\u00d0\u00b0 \u00d1\u0081\u00d0\u00b0\u00d0\u00b9\u00d1\u201a\u00d0\u00b5 \u00d1\u20ac\u00d0\u00b0\u00d0\u00b7\u00d0\u00bc\u00d0\u00b5\u00d1\u2030\u00d0\u00b0\u00d1\u017d\u00d1\u201a\u00d1\u0081\u00d1\u008f \u00d1\u0081